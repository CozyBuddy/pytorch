import pandas as pd

df = pd.read_csv('study.csv')

print(df.head())

from statsmodels.formula.api import ols

model = ols('score ~ study_hours + material_type',df).fit()

print(model.summary())

df = pd.read_csv('health_survey.csv')

print(df.head())

from statsmodels.formula.api import logit

model = logit('disease ~ age + bmi',df).fit()

print(model.summary())

import numpy as np

print(model.params['bmi'])

print(np.exp(model.params['bmi']))

print(model.llf * -2)